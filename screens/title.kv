<TitleLabel@Label>:
    size_hint_y: None
    height: '80sp'
    Label:
    	text: 'TabMachine'

<TitleScreen>:
    FloatLayout:
    BoxLayout:
        id: title_layout
        size: self.parent.size
        Button:
            id: open_tab
            text: 'Open Tab'
            pos: (475, 250)
            size_hint: (.30, .50)
            on_press: root.manager.current = 'ViewScreen'
        Button:
            id: create_tab
            text: 'Create Tab'
            pos: (1475, 250)
            size_hint: (.30, .50)
            on_press: root.manager.current = 'CreateScreen'
